# Open Interpreter shortcuts
alias oi='interpreter'
alias oic='interpreter --config'
alias oip='interpreter "$1" | less'
alias oie='interpreter "$1" > "${2:-output.py}"'

# Jupyter Notebook shortcuts
alias jnb='jupyter notebook'

# Local knowledge base
alias kbadd='cp "$1" ~/kb/'
alias kbls='ls ~/kb'
alias kbcat='cat ~/kb/"$1"'

# Local CI/CD pipeline
alias cit='bash ~/ci-cd/run-tests.sh'

# tmux
alias dev='tmux new-session -d -s dev \; split-window -v -p 30 \; split-window -h \; attach'
